<Window x:Class="AutoPiano.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:AutoPiano"
        mc:Ignorable="d"
        WindowStyle="None"
        Background="#1e1e1e"
        Title="MainWindow" Height="810" Width="1440">
    <Window.Resources>
        <Style TargetType="Ellipse">
            <Setter Property="Fill" Value="Cyan"/>
            <Setter Property="Width" Value="10"/>
            <Setter Property="Height" Value="10"/>
        </Style>
    </Window.Resources>
    <Viewbox>
        <Grid Height="810" Width="1440">
            <Ellipse x:Name="Trail"/>
            <local:Sidebar Margin="0,157,1200,53" x:Name="TabA"/>
            <local:SuperMenu Margin="0,0,0,750" x:Name="Selects"/>
        </Grid>
    </Viewbox>
</Window>
